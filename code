#include <stdio.h>

int main()
{
    int a,b;
    char op;
    scanf("%d",&a);
    scanf("%c",&op);
    scanf("%d",&b);
    switch(op)
    {
        case '+':
        printf("%d+%d=%d",a,b,a+b);
        break;
        case '-':
        printf("%d-%d=%d",a,b,a-b);
        break;
        case '*':
        printf("%d*%d=%d",a,b,a*b);
        break;
        case '/':
        if(b=0)
        {
            printf("not defined");
        }
        else
        {
            printf("%d/%d=%d",a,b,a/b);
        }
        break;
        default:
        printf("Invalid");
    }
    return 0;
}
